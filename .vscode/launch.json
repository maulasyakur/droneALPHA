{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug TestBuild",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/TestBuild",
        "args": [
          "250",
          "5",
          "distance_data.csv",
          "priority_provinces.txt",
          "weather_restricted_provinces.txt"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build TestBuild"
      }
    ]
  }
  